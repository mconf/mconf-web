<div class="agenda_divider margin-up">
  <div class="divider_full_line">
  	<ul class="group_links group_agenda_links">
  	  <li><%=link_to_function(image_tag("icons/pencil.png", :alt => t('edit'), :title => t('edit')) , "return false;", :class=>"edit_divider", :name=>agenda_divider.id.to_s) if @event.authorize?(:update, :to => current_user) %></li>
      <li><%=link_to(image_tag("icons/cancel.png", :alt => t('delete.one')) , space_event_agenda_agenda_divider_path(@space, @event, agenda_divider), :method => :delete, :title => t('agenda.del_entry'), :confirm => t('delete.confirm', :element=>agenda_divider.title)) if @event.authorize?(:update, :to => current_user) %></li>
	</ul>
	<span class="divider_title">
	  <%=agenda_divider.title%>  
	</span>    	
  </div>
</div>
